<Window x:Class="LaserControl.Tests.TestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TestWindow" Height="100" Width="300" ResizeMode="NoResize" Topmost="True" WindowStartupLocation="CenterScreen" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <ComboBox x:Name="SelectedTest"  Margin="50,0,50,0" HorizontalAlignment="Stretch" VerticalAlignment="Center">
            <ComboBoxItem Content="0) ImgButton + En/Disable Test" />
            <ComboBoxItem Content="1) Enable Move Move Home Disable" />
            <ComboBoxItem Content="2) Tab Control" />
            <ComboBoxItem Content="3) Test Script V2 (FAK)" />
            <ComboBoxItem Content="4) Test Script V2 (Move 800 100)" />
        </ComboBox>
        <Button Content="Start" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Width="75" Height="25" Click="Button_Click"/>
    </Grid>
</Window>
